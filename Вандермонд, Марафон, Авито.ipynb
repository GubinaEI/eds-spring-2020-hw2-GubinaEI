    "import requests\n",
    "from bs4 import BeautifulSoup as bs\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "def GetData():\n",
    "    print( '–≤–≤–µ–¥–∏—Ç–µ –ø–æ–∏—Å–∫–æ–≤–æ–π –∑–∞–ø—Ä–æ—Å: ' )\n",
    "    search = input()\n",
    "    print( '–≤–≤–µ–¥–∏—Ç–µ —Å—Ç—Ä–∞–Ω–∏—Ü—É –¥–ª—è –ø–æ–∏—Å–∫–∞: ' )\n",
    "    page = int( input() )\n",
    "    return [search, page]\n",
    "\n",
    "\n",
    "def SoupToDB(list):\n",
    "    column_names = ['title', 'link', 'price', 'metroName', 'metroDist']\n",
    "    df = pd.DataFrame( columns=column_names )\n",
    "    for i in list:\n",
    "        title = i.find( 'div', class_='snippet-title-row' ).find( 'a', class_='snippet-link' )[\n",
    "            'title']\n",
    "        link = i.find( 'div', class_='snippet-title-row' ).find( 'a', class_='snippet-link' )[\n",
    "            'href']\n",
    "        price = i.find( 'span', class_='snippet-price' ).text\n",
    "        temp = price.split( ' ' )\n",
    "        price = ''\n",
    "        for j in temp:\n",
    "            try:\n",
    "                temp2 = int( j )\n",
    "                price += j\n",
    "            except ValueError:\n",
    "                pass\n",
    "        if (price == ''):\n",
    "            price = 'None'\n",
    "        try:\n",
    "            metroName = i.find( 'span',\n",
    "                                class_='item-address-georeferences-item__content' ).text\n",
    "        except (TypeError, AttributeError):\n",
    "            metroName = 'None'\n",
    "        try:\n",
    "            metroDist = i.find( 'span',\n",
    "                                class_='item-address-georeferences-item__after' ).text\n",
    "        except (TypeError, AttributeError):\n",
    "            metroDist = 'None'\n",
    "\n",
    "        df = df.append( {'title': title, 'link': link, 'price': price, 'metroName': metroName, 'metroDist': metroDist},\n",
    "                        ignore_index=True )\n",
    "        # print(title + ' ' + price + ' ' + metroName + ' ' + metroDist)\n",
    "    return df\n",
    "\n",
    "\n",
    "def Parse(search, page):\n",
    "    siteTitle = 'https://www.avito.ru/'\n",
    "    cityLinkTitle = 'moskva'\n",
    "    page = requests.get(\n",
    "        siteTitle + cityLinkTitle + '?q=' + search + '&p=' + str( page ) )\n",
    "    soup = bs( page.text, \"lxml\" )\n",
    "    soup.prettify()\n",
    "    annList = soup.find_all( 'div', {'class': 'item_table-wrapper'} )\n",
    "    # print(len(trade_list)) #–∫–æ–ª-–≤–æ –æ–±—ä—è–≤–ª–µ–Ω–∏–π –Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü–µ\n",
    "    df = SoupToDB( annList )\n",
    "    return df\n",
    "\n",
    "\n",
    "def Output(result):\n",
    "    print( result )\n",
    "\n",
    "\n",
    "def main():\n",
    "    data = GetData()\n",
    "    result = Parse( data[0], data[1] )\n",
    "    Output( result )\n",
    "\n",
    "main()\n"
