    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "n_of_maraphons = 10 ^ 4\n",
    "n_of_hours = 100\n",
    "\n",
    "debug = False\n",
    "\n",
    "\n",
    "def bear():\n",
    "    return np.random.exponential( 1, (n_of_maraphons, n_of_hours) )\n",
    "\n",
    "def pig():\n",
    "    return np.random.normal( 1, 1, (n_of_maraphons, n_of_hours) )\n",
    "\n",
    "def rabbit():\n",
    "    return np.random.poisson( 1, (n_of_maraphons, n_of_hours) )\n",
    "\n",
    "def donkey():\n",
    "    return 2 * np.random.binomial( 1, 0.5, (n_of_maraphons, n_of_hours) )\n",
    "\n",
    "\n",
    "runners = (bear(), pig(), rabbit(), donkey())\n",
    "speeds = np.stack( runners, axis=1 )\n",
    "if debug:\n",
    "    print( \"\\nspeeds:\" )\n",
    "    print( speeds )\n",
    "distances = speeds.cumsum( axis=2 )\n",
    "if debug:\n",
    "    print( \"\\ndistances:\" )\n",
    "    print( distances )\n",
    "results = distances[:, :, -1]\n",
    "if debug:\n",
    "    print( \"\\nresults:\" )\n",
    "    print( results )\n",
    "winners = np.argmax( results, axis=1 )\n",
    "if debug:\n",
    "    print( \"\\nwinners (0=bear, 1=pig, 2=rabbit, 3=donkey):\" )\n",
    "    print( winners )\n",
    "if debug:\n",
    "    runners, n_of_wins = np.unique( winners, return_counts=True )\n",
    "    wins = dict( zip( runners, n_of_wins ) )\n",
    "    print( \"\\nwins:\" )\n",
    "    print( wins )\n",
    "\n",
    "x = [\"bear\", \"pig\", \"rabbit\", \"donkey\"]\n",
    "y = [np.count_nonzero( winners == 0 ),\n",
    "     np.count_nonzero( winners == 1 ),\n",
    "     np.count_nonzero( winners == 2 ),\n",
    "     np.count_nonzero( winners == 3 )]\n",
    "\n",
    "fig, ax = plt.subplots()\n",
    "ax.bar( x, y )\n",
    "plt.show()"
   ]
