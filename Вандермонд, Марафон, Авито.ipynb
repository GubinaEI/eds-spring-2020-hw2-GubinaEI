    "import numpy as np\n",
    "import pprint as pp\n",
    "\n",
    "\n",
    "def generate_vander_matrix1(arr):\n",
    "    return [[x**i for i in range(len(arr)) ] for x in arr]\n",
    "\n",
    "\n",
    "def generate_vander_matrix2(arr):\n",
    "    matrix = np.zeros( (len( arr ), len( arr )) )\n",
    "    matrix[:, 0] = 1\n",
    "    matrix[:, 1:] = arr[:, None]\n",
    "    return np.multiply.accumulate( matrix, axis=1 )\n",
    "\n",
    "\n",
    "input_numbers = list( map( int, input().split() ) )\n",
    "input_array = np.array( input_numbers )\n",
    "setup = \"from __main__ import generate_vander_matrix1, generate_vander_matrix2, input_array\"\n",
    "\n",
    "pp.pprint( generate_vander_matrix2( input_array ) )\n"
